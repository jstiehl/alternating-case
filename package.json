{
  "name": "@jstiehl/alternating-case",
  "version": "1.0.1",
  "description": "Convert text to alternating uppercase and lowercase letters",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist/esm",
    "dist/types"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "test": "npm run build && vitest",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "string",
    "text",
    "case",
    "utility"
  ],
  "author": "James Stiehl <jstiehl@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
